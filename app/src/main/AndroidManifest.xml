<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edmt.dev.androidcamera2api">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Account"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LupaPassword"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ReaderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterApp"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Menu"
            android:screenOrientation="portrait" />
        <activity android:name=".maintenance" />
        <activity android:name=".maintenceinfo" />
        <activity android:name=".DetailBedcleaning" />
        <activity android:name=".Bedcleaning" />
        <activity android:name=".Bedcleaningtimer" />

        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />

        <activity android:name=".PasienQR" />
        <activity android:name=".ObatQR" />
        <activity android:name=".AreaCleaningTimer"></activity>
    </application>

</manifest>